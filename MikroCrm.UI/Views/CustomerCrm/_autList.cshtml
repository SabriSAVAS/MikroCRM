@model List<MikroCrm.UI.ViewModel.CustomerCrmModel.CustomerAutListViewModel>
<table class="table table-bordered table-striped table-actions">
    <thead>
        <tr>
           
            <th>Yetkili ad</th>

            
            <th>Yetkili soyad</th>

            <th class="text-center"><span class="glyphicon glyphicon-cog"></span> </th>
        </tr>
    </thead>
    @if (Model != null)
    {
        if (Model.Count > 0)
        {
            foreach (var item in Model)
            {
                <tbody>
                    <tr>
                     
                        <td>
                        <label id="aut_@item.Id" data-id="@item.YetkiliAd">@item.YetkiliAd</label>
                        </td>

                        <td>@item.YetkiliSoyad</td>
                        <td class="text-center">
                            <button onclick="customerautselect(@item.Id)" class="btn btn-default"><span class="glyphicon glyphicon-ok-sign"></span></button>
                        </td>
                    </tr>
                </tbody>
            }
        }
    }

</table>

